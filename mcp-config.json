{
    "mcpServers": {
        "meow-notifier": {
            "command": "npx",
            "args": [
                "-y",
                "meow-notifier"
            ],
            "env": {
                "MEOW_NICKNAMES": "[\"test1\", \"test2\", \"test3\"]"
            },
            "timeout": 60,
            "alwaysAllow": [
                "send_notification"
            ]
        }
    }
}